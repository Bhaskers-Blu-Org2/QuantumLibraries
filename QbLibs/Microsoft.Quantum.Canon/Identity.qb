namespace Microsoft.Quantum.Canon {

    operation I(target : Qubit) : () {
        Body {}
        Adjoint self
        Controlled auto
        Controlled Adjoint auto
    }

    operation NoOp<'T>(target : 'T) : () {
        Body {}
        Adjoint self
        Controlled auto
        Controlled Adjoint auto
    }

    function Identity<'T>(value : 'T) : 'T {
        return value
    }

}
